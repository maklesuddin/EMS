
@model Models.SearchCriteria.CitySearchCriteria
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Share/_LayoutShare.cshtml";
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-12">
                <!-- Chat box -->
                <div class="box box-success">
                   
                    <div class="box-body">
                        <!-- Form Start -->

                        <p>
                            @Html.ActionLink("Create New", "Entry")
                        </p>
                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Name)
                                </th>
                                @*<th>
                                    @Html.DisplayNameFor(model => model.CountryId)
                                </th>*@

                                <th></th>
                            </tr>

                            @foreach (var item in Model.City)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Name)
                                    </td>
                                    @*<td>
                                        @Html.DisplayFor(modelItem => item.CountryId)
                                    </td>*@

                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                        @Html.ActionLink("Details", "Index", new { id = item.Id }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                    </td>
                                </tr>
                            }

                        </table>

                        <!--Form Closed-->
                    </div>
                </div>
            </section>
        </div>
        <!-- /.row (main row) -->
    </section>
    <!-- /.content -->
</div>



























