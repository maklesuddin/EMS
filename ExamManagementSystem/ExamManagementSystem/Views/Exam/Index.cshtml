
@model Models.SearchCriteria.ExamSearchCriteria

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Share/_LayoutShare.cshtml";
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-12">
                <!-- Chat box -->
                <div class="box box-success">
                    <div class="box-header">
                        <h3 class="box-title">Exam Entry</h3>
                    </div>
                    <div class="box-body">
                        <!-- Form Start -->

                        <p>
                            @Html.ActionLink("Create New", "Entry")
                        </p>
                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.OrganizationId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CourseId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.ExamTypeId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Code)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Topic)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.FullMarks)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.QuestionId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Answer)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IsDeleted)
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model.ExamlisList)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Organization.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Course.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ExamType.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Code)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Topic)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FullMarks)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Question.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Answer)
                                    </td>

                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                                    </td>
                                </tr>
                            }

                        </table>
                        <!--Form Closed-->
                    </div>
                </div>
            </section>
        </div>
        <!-- /.row (main row) -->
    </section>
    <!-- /.content -->
</div>


